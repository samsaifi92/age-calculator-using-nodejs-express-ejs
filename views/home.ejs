<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Year of Birth</title>
</head>

<body>
    <%= Day %>
        <h1>Year of Birth Calculator </h1>
        <span>
            <% if (err) { %>
                <li>
                    <%= err %>
                </li>
                <% } %>
        </span>
        <form action="/" method="post">
            <label for="YearOfBirth">How old are you?</label>
            <input type="text" id="YearOfBirth" name="currentAge" placeholder="Your age">
            <button type="submit">Find Year</button>
        </form>
        <ul>
            <% BirthYear.forEach(function(year) { %>

                <li>
                    <%= year %>
                </li>

                <% }); %>
        </ul>

</body>

</html>